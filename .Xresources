URxvt.scrollBar: false
URxvt*font: xft:DejaVu Sans Mono:pixelsize=13.5
URxvt*letterSpace: -2
URxvt*fading: 20
URxvt*cursorBlink: true
Xft.dpi:        96
Xft.antialias:  true
Xft.hinting:    true
Xft.rgba:       rgb
Xft.hintstyle:  hintfull

